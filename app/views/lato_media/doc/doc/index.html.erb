<%
  row = cell(:elements, :row).new
  block = cell(:elements, :block).new(class: 'xs-12 sm-12 md-12')
%>

<!-- PRESENTATION -->
<%=raw row.open %>

  <%=raw block.open %>

    <%=raw cell(:elements, :title).new(label: 'Lato Media Documentation') %>

  <%=raw block.close %>

<%=raw row.close %>
<!-- / PRESENTATION -->

<!-- CELLS SECTION -->
<%=raw row.open %>

  <%=raw block.open %>

    <%=raw cell(:elements, :title).new(label: 'Cells', size: 2) %>

    <%=raw cell(:elements, :button).new(label: 'Elements Mediaselector', url: lato_media.doc_cells_elements_mediaselector_path) %>
    <%=raw cell(:elements, :button).new(label: 'Inputs Media', url: lato_media.doc_cells_inputs_media_path) %>

  <%=raw block.close %>

<%=raw row.close %>
<!-- / CELLS SECTION -->

<%=raw row.close %>
<!-- / CELLS SECTION -->